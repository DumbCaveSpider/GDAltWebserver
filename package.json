{
  "name": "gdalternativeweb-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
  "build": "node build.js",
    "start": "node -e \"require('child_process').spawnSync('./bin/gdaltweb.exe', {stdio: 'inherit', shell: true})\"",
  "predev": "npm run build",
  "prod": "node build.js && dotenv -e .env -- node start.js",
    "prod:printenv": "dotenv -e .env -- node -e \"console.log(process.env.PORT)\""
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
